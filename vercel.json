{
    "version": 2,
    "builds": [
        {
            "src": "./server.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "scripts": [
        {
        "start": "gunicorn main:app --bind 0.0.0.0:8000 server:server"
        }
    ]
}
